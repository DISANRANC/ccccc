    <script>
        new Vue({
            el: '#app',
            data() {
                return {
                    productList:"", //所有数据
                    // name:"",
                    totalPage: "", // 统共页数，默认为1
                    currentPage: 1, //当前页数 ，默认为1
                    pageSize:1, // 每页显示数量
                    totalCount:"",
                    currentPageData: [] //当前页显示内容
                };
            },
            mounted() {
                // 计算一共有几页
                setTimeout(()=>{
                    console.log(this.totalPage); 
                    this.totalPage = Math.ceil(this.totalCount / this.pageSize);
                    // 计算得0时设置为1
                    console.log(this.totalCount )
                    this.totalPage = this.totalPage == 0 ? 1 : this.totalPage;
                    this.setCurrentPageData();
                },100)
                
            },
            created(){
                this.getList();
            },
            methods: {
                // 设置当前页面数据，对数组操作的截取规则为[0~pageSize],
                setCurrentPageData() {
                    let begin = (this.currentPage - 1) * this.pageSize;
                    console.log(begin)
                    let end = this.currentPage * this.pageSize;
                    this.currentPageData = this.productList.slice(
                        begin,
                        end
                    );
                    console.log(this.currentPageData)
                },
                //上一页
                prevPage() {
                    console.log(this.currentPage);
                    console.log(this.productList.length);
                    if (this.currentPage == 1) return;
                
                    this.currentPage--;
                    console.log(this.currentPage)
                    this.getList()
                },
                // 下一页
                nextPage() {
                    console.log(this.totalPage); 
                    if (this.currentPage == this.totalPage)return ;
                    this.currentPage++;
                    console.log(this.currentPage)
                    // this.setCurrentPageData();
                    this.getList()
                },
                getList(){
                    this.$http.get(
                        'http://192.168.100.92/api/payitem/findPayitemPage',
                        {
                            'params':{
                                limit:this.pageSize,
                                page:this.currentPage
                            }
                        }).then(res => {
                            console.log(res)
                            if (res.data.code === 0) {
                                //msg等于回调函数返回的res(值)
                                this.pageSize = res.data.data.pageSize//每页数量
                                this.totalPage = res.data.data.totalPage//总页数
                                this.currentPage = res.data.data.currPage//当前页数
                                this.totalCount = res.data.data.totalCount//总数量

                                this.productList=res.data.data.list;
                                //在打印台测试打印，无误后一定要删除
                                console.log(this.productList,this.pageSize,this.totalPage); 
                            }
                        }).catch(err => {
                            console.error(err)
                        })
                },
                popup(){
                    $(".mask").fadeIn()
                    $(".ticket-box").fadeIn()
                }
            }
        })	
    </script>
